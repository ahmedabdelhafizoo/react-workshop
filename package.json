    ]
  },
  "dependencies": {
    "apexcharts": "^3.36.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "jest": "^29.4.1",
    "vite": "^4.0.0"
  }
}
